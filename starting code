//code for workcube
#include <PRIZM.h>   //link up the library
PRIZM prizm;         //declare object (reference the prizm)

void setup(){
  prizm.PrizmBegin();
}

void loop (){
  prizm.setMotorPower(2,100);  //100% speed
  delay(2000);
  prizm.setGreenLED;
  prizm.setRedLED;
  prizm.setMotorPower(2,125);  //125 means BRAKE
  delay(2000)
  
}
